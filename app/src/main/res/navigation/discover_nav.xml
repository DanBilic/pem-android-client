<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/discover_nav"
    app:startDestination="@id/discoverFragment">

    <fragment
        android:id="@+id/discoverFragment"
        android:name="com.example.pemapp.ui.discover.discoverFragment"
        android:label="fragment_discover"
        tools:layout="@layout/fragment_discover" >
        <action
            android:id="@+id/action_discoverFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
        <action
            android:id="@+id/action_discoverFragment_to_navigation3"
            app:destination="@id/moments_nav_graph" />
        <action
            android:id="@+id/action_discoverFragment_to_navigation4"
            app:destination="@id/profile_nav_graph" />
    </fragment>

    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.pemapp.ui.discover.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" >
        <action
            android:id="@+id/action_detailFragment_to_discoverFragment"
            app:destination="@id/discoverFragment" />
    </fragment>
    <navigation android:id="@+id/moments_nav_graph"
        app:startDestination="@id/moments_nav">
        <include app:graph="@navigation/moments_nav" />
        <action
            android:id="@+id/action_global_discoverFragment"
            app:destination="@id/discoverFragment" />
    </navigation>
    <navigation android:id="@+id/profile_nav_graph"
        app:startDestination="@id/profile_nav">
        <include app:graph="@navigation/profile_nav" />
        <action
            android:id="@+id/action_global_discoverFragment2"
            app:destination="@id/discoverFragment" />
    </navigation>

</navigation>